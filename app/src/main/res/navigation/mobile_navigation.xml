<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.shopexpress1.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/categories"
        android:name="com.example.shopexpress1.ui.dashboard.DashboardFragment"
        android:label="Account"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/account"
        android:name="com.example.shopexpress1.ui.account.AccountFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/account_fragment" />

    <fragment
        android:id="@+id/support"
        android:name="com.example.shopexpress1.ui.SupportFragment"
        android:label="About us"
        tools:layout="@layout/fragment_support" />
</navigation>